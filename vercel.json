{
    "version": 2,
    "builds": [
        {
            "src": "banner_apis/wsgi.py",
            "use": "@vercel/python ",
            "config":{
                "maxLambdaSize":"15mb",
                "runtime":"python3.10.6"
            }
        },
        {
            "src":"build.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"staticfiles_build"
            }
        }
        
    ],
    "routes": [
        {
            "src":"/static/(.*)",
            "dest":"static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "banner_apis/wsgi.py"
        }
    ]
}