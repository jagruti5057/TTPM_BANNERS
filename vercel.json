{
    "version": 2,
    "builds": [
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
          "maxLambdaSize": "15mb"
        }
      },
      {
        "src": "banner_apis/wsgi.py",
        "use": "@vercel/python-builder",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.10"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/banner_apis/wsgi.py"
      }
    ]
  }
  